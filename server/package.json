{
  "name": "repostats-server",
  "version": "0.0.1",
  "description": "Analyze commit, repository and review data.",
  "repository": "https://github.com/holmari/github-gerritstats",
  "author": "Lasse Holmstedt",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,scss}\" --list-different",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/parse-link-header": "^1.0.0",
    "async": "^3.1.0",
    "axios": "^0.21.1",
    "axios-rate-limit": "^1.3.0",
    "axios-retry": "^3.2.4",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "github-api": "^3.3.0",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "luxon": "^1.25.0",
    "parse-link-header": "^1.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "slugify": "^1.4.6",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-flash": "0.0.1",
    "@types/express-session": "^1.15.14",
    "@types/jest": "^24.0.18",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.141",
    "@types/lusca": "^1.6.1",
    "@types/luxon": "^1.25.1",
    "@types/node": "^12.7.8",
    "@types/nodemailer": "^6.2.1",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  },
  "main": "src/server.ts",
  "private": false
}
